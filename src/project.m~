
Cl = .4;
Cd = .4;
m = 52; %kg 
l1 = .75; %meters
l2 = .75; %meters

A = [(Cl/2 - Cd)/m, Cl/(2*m), 0, Cl/m;
     -Cl/2*m, -(Cl/2 + Cd)/m, 0, Cl/m;
     0, 0, 0, 0;
     0, 0, 1, 0];
 
B = [0, 0;
     1/m, 1/m;
     -l1/m, l2/m;
     0, 0];
     

 
 
 
P = [B, A*B, A*A*B, A*A*A*B];
P = unique(P', 'rows', 'stable')';


mu = 2;

Minv = inv(M);

T = [Minv(1,:); ...
     Minv(2,:)*A; ...
     Minv(3,:)*A*A; ...
     Minv(4,:)*A*A*A ];
 
Tinv = inv(T);

Abar = T*A*Tinv;








